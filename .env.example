# MOBIUS Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Key (REQUIRED for AI features)
# Get your key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Image Extractor API Key (OPTIONAL)
# Get your key from: https://extract.pics/
IMAGE_EXTRACTOR_API_KEY=your_image_extractor_key_here

# Server Configuration
PORT=5001

# Output Directory (where generated content is stored)
OUTPUT_DIR=./src/api/uploads/MobiusGames

# Development Mode (optional)
NODE_ENV=development
